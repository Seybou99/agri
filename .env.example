# Configuration de l'application
# Phase 1 : mettre l'URL de l'API Vercel (ex. https://ton-projet.vercel.app) pour utiliser /api/v1/*
API_URL=https://api.example.com
API_KEY=your_api_key_here

# Environnement
ENV=development

# Firebase Configuration
FIREBASE_API_KEY=your_firebase_api_key
FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
FIREBASE_PROJECT_ID=your-project-id
FIREBASE_STORAGE_BUCKET=your-project.appspot.com
FIREBASE_MESSAGING_SENDER_ID=123456789
FIREBASE_APP_ID=1:123456789:web:abcdef

# Supabase (Phase 1 - persistance diagnostics)
# Créer un projet sur https://supabase.com, puis exécuter docs/DB_SCHEMA_SUPABASE_MINIMAL.sql dans SQL Editor
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# APIs externes
OPENWEATHER_API_KEY=your_openweather_api_key
# AgroMonitoring (surveillance agricole, NDVI, météo parcelle) — https://home.agromonitoring.com → Clés d'API
AGROMONITORING_API_KEY=your_agromonitoring_api_key
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Google Earth Engine (backend / Cloud Functions uniquement)
# Chemin vers le JSON du compte de service (jamais dans l'app mobile ni dans Git)
GOOGLE_APPLICATION_CREDENTIALS=./path/to/your-service-account.json
GEE_PROJECT_ID=my-project-agricol
